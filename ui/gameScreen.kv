<GameScreen>:
    MDScreen:
        md_bg_color: [0.9,0.9,0.9,1]
        MDBoxLayout:
            orientation: "vertical"

            MDLabel:
                id: header
                font_size: 36
                font_name: "CourierNew"
                color: [0,0,0,1]
                bold: True

            MDBoxLayout:
                orientation: "vertical"
                id: center
                size_hint: 1,1

            MDBoxLayout:
                id: actionText
                orientation: "vertical"
                size_hint: 1,.2

            MDTextButton:
                id: nextPageButton
                on_release: root.nextPage()
                text: "nextPageText"
                font_name: "CourierNew"
                bold: True
                theme_text_color: "Custom"
                text_color: root.nextPageButtonColor()

            MDFloatingActionButtonSpeedDial:
                id: FABSD
                data: root.dataFABSD()
                rotation_root_button: True
                callback: root.callbackFABSD